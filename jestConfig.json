{
  "globals": {
    "ts-jest": {
      "tsconfig": "ts/tsconfig.json",
      "diagnostics": false
    }
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "testPathIgnorePatterns": [
    "<rootDir>/build/",
    "<rootDir>/docs/",
    "<rootDir>/lib/",
    "<rootDir>/src/",
    "<rootDir>/node_modules/"
  ],
  "preset": "ts-jest/presets/js-with-ts",
  "testMatch": null
}